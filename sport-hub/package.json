{
  "name": "sport-hub",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "db:local": "docker-compose up -d dynamodb-local",
    "db:setup": "DYNAMODB_LOCAL=true tsx src/lib/db-setup.ts",
    "db:seed": "DYNAMODB_LOCAL=true tsx src/lib/seed-local-db.ts seed",
    "db:reset": "DYNAMODB_LOCAL=true tsx src/lib/seed-local-db.ts reset",
    "db:clear": "DYNAMODB_LOCAL=true tsx src/lib/seed-local-db.ts clear",
    "db:count": "DYNAMODB_LOCAL=true tsx src/lib/seed-local-db.ts count",
    "db:stop": "docker-compose stop dynamodb-local",
    "db:clean": "docker-compose down -v",
    "test:local": "DYNAMODB_LOCAL=true pnpm run dev"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.821.0",
    "@aws-sdk/credential-providers": "^3.840.0",
    "@aws-sdk/lib-dynamodb": "^3.821.0",
    "@tanstack/react-table": "^8.21.2",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "formik": "^2.4.6",
    "google": "link:next/font/google",
    "next": "15.3",
    "next-auth": "5.0.0-beta.30",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-circle-flags": "^0.0.23",
    "react-dom": "^19.0.0",
    "react-social-icons": "^6.25.0",
    "recharts": "^2.15.3",
    "tailwind-merge": "^3.2.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.0.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "tailwindcss": "^4.0.14",
    "tsx": "^4.7.1",
    "typescript": "^5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
